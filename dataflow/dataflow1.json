{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Moviesds",
						"type": "DatasetReference"
					},
					"name": "movies"
				},
				{
					"name": "moviesDB"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          movieId as string,",
				"          title as string,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> movies",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> moviesDB"
			]
		}
	}
}